{
  "name": "zichange-api",
  "version": "0.0.1",
  "description": "Zichange API",
  "author": "Zichange Moscow IT",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main.js",
    "tsc:watch": "rimraf dist && tsc --watch",
    "tsc:build": "rimraf dist && tsc",
    "webpack": "webpack --config webpack.config.js",
    "start:dev:hmr": "node hmr/server",
    "start:dev:hmr:debug": "node --inspect-brk hmr/server",
    "start:dev:nodemon": "nodemon --watch dist/**/*.js --exec node dist/main.js",
    "start:dev:nodemon:debug": "nodemon --watch dist/**/*.js --exec node --inspect-brk dist/main.js",
    "test": "jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "docker:redis": "docker run --rm -it -p 6379:6379 redis",
    "docker:build": "docker build -t zichange.eu/zichange-api:latest .",
    "docker:run:dev": "docker run --rm -it --env-file=./.env -e DB_TYPEORM_HOST=host.docker.internal -e REDIS_URL=redis://host.docker.internal:6379 -p 3333:3333 zichange.eu/zichange-api:latest"
  },
  "dependencies": {
    "7zip-bin": "^4.1.0",
    "@google-cloud/storage": "^2.4.1",
    "@nestjs/common": "^5.3.9",
    "@nestjs/core": "^5.3.10",
    "@nestjs/cqrs": "^5.1.1",
    "@nestjs/microservices": "^5.3.8",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/testing": "^5.3.1",
    "@nestjs/typeorm": "^5.2.2",
    "@nestjs/websockets": "5.3.1",
    "@types/pino": "^4.16.0",
    "archiver": "^3.0.0",
    "axios": "^0.18.0",
    "bignumber.js": "^8.0.1",
    "canvas": "^2.5.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "connect-typeorm": "1.0.4",
    "cron": "^1.7.1",
    "crypto-js": "^3.1.9-1",
    "dotenv-safe": "^6.0.0",
    "express-session": "1.15.6",
    "fastify-formbody": "^2.0.0",
    "form-data": "^2.3.3",
    "gt3-sdk": "^2.0.0",
    "mailgun-js": "^0.22.0",
    "marko": "4.16.13",
    "max-listeners-exceeded-warning": "^0.0.1",
    "mistertango": "^0.1.1",
    "moment": "^2.22.2",
    "node-7z": "^1.0.1",
    "nodemailer": "^5.0.0",
    "nonce": "^1.0.4",
    "pdfjs-dist": "^2.0.943",
    "pdfmake": "^0.1.38",
    "pg": "^7.7.1",
    "pino": "^4.17.5",
    "qrcode": "^1.2.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "request-ip": "^2.1.1",
    "rxjs": "^6.0.0",
    "shortid": "^2.2.8",
    "speakeasy": "^2.0.0",
    "svg-captcha": "^1.3.11",
    "typeorm": "^0.2.9",
    "typeorm-static": "^1.1.0",
    "uniqid": "^5.0.3",
    "uuid": "^3.3.2",
    "wallet-address-validator": "^0.2.4"
  },
  "devDependencies": {
    "@types/cron": "^1.7.1",
    "@types/express": "^4.0.39",
    "@types/express-session": "^1.15.10",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^23.3.10",
    "@types/mailgun-js": "^0.16.3",
    "@types/node": "^9.3.0",
    "@types/nodemailer": "^4.6.5",
    "@types/request-ip": "^0.0.33",
    "@types/socket.io": "^1.4.36",
    "@types/supertest": "^2.0.4",
    "jest": "^23.6.0",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.1.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.10.0",
    "typescript": "^3.2.2",
    "utility-types": "^2.1.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": "spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
